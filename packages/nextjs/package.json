{
  "name": "@tern-secure-node/nextjs",
  "version": "0.0.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TernSecure/auth-node.git",
    "directory": "packages/nextjs"
  },
  "bugs": {
    "url": "https://github.com/TernSecure/auth-node/issues"
  },
  "homepage": "https://github.com/TernSecure/auth-node#readme",
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm clean && tsup",
    "build:add": "tsc -p tsconfig.add.json",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "bench": "vitest bench"
  },
  "keywords": [
    "firebase",
    "authentication",
    "nextjs",
    "auth",
    "typescript"
  ],
  "author": "Pacifique Kamugisha",
  "license": "ISC",
  "description": "Firebase authentication solution for Next.js applications",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "admin",
    "server"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./server": {
      "types": "./dist/types/server/index.d.ts",
      "import": "./dist/esm/server/index.js",
      "require": "./dist/cjs/server/index.js"
    },
    "./server/node": {
      "types": "./dist/types/server/node/index.d.ts",
      "import": "./dist/esm/server/node/index.js",
      "require": "./dist/cjs/server/node/index.js"
    },
    "./admin": {
      "types": "./dist/types/app-router/admin/index.d.ts",
      "import": "./dist/esm/app-router/admin/index.js",
      "require": "./dist/cjs/app-router/admin/index.js"
    }
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@tern-secure-node/backend": "workspace:*",
    "@tern-secure-node/react": "workspace:*",
    "@tern-secure/shared": "1.3.0-canary.v20251028151628",
    "@tern-secure/types": "1.1.0-canary.v20251028151628",
    "cookie": "1.0.2",
    "jose": "^5.9.6",
    "server-only": "^0.0.1",
    "tslib": "2.4.1"
  },
  "peerDependencies": {
    "firebase": "^12.0.0",
    "next": "^16.0.0",
    "react": "^19",
    "react-dom": "^19"
  },
  "engines": {
    "node": ">=20"
  }
}